{
    "name": "Zeon",
    "description": "New way to browse internet.",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "permissions": [
      "tabs",
      "storage",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon-34.png"
    },
    "icons": {
        "128": "images/icon-128.png"
      }
  }
  